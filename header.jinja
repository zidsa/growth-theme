                    </div>
                  </div>
                </div>
              </div>
            </div>
          </el-dialog-panel>
        </div>
      </dialog>
    </el-dialog>
    <div class="flex items-center gap-8">
      {% if settings.logo_desktop or settings.logo_mobile %}
        <img
          width="106"
          height="44"
          src="{{ settings.logo_desktop | default(settings.logo_mobile) | default(store.logo) }}"
          class="hidden md:block"
          alt="{{ store.name }}"
        />
        <img
          width="106"
          height="44"
          src="{{ settings.logo_mobile | default(settings.logo_desktop) | default(store.logo) }}"
          class="block md:hidden"
          alt="{{ store.name }}"
        />
      {% else %}
        <img
          width="106"
          height="44"
          src="{{ store.logo }}"
          alt="{{ store.name }}"
        />
      {% endif %}
      <ul class="hidden items-center gap-6 md:flex">
        {% for menu in store.settings.menus.main_menu.items %}
          <li>
            {% if menu.items | length > 0 %}
              <el-dropdown class="inline-block">
                <button class="text-primary flex items-center gap-x-2 py-2 text-[16px] leading-[24px]">
                  {{ menu.label }}
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    width="16"
                    height="16"
                    viewBox="0 0 16 16"
                    class="text-primary"
                    fill="none"
                  >
