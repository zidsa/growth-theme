{% include 'components/header-announcement-bar.jinja' %}
<header class="border-b border-[#F2F2F2] px-4 py-3 md:px-16 md:py-4">
  <nav class="theme-container flex items-center justify-between">
    <el-dialog class="group contents">
      <button
        command="show-modal"
        commandfor="mobile-drawer"
        class="text-primary relative h-10 w-10 p-2 group-data-[transition]:pointer-events-none focus:outline-none md:hidden"
        aria-label="Toggle menu"
      >
        <span
          aria-hidden="true"
          class="absolute block h-0.5 w-5 transform bg-current transition duration-500 ease-in-out group-data-[closed]:-translate-y-1.5 group-data-[enter]:rotate-45 group-data-[leave]:!rotate-0 group-[:not([open])]:-translate-y-1.5 group-[[open]]:rotate-45"
        ></span
        ><span
          aria-hidden="true"
          class="absolute block h-0.5 w-5 transform bg-current transition duration-500 ease-in-out group-data-[enter]:opacity-0 group-data-[leave]:!opacity-100 group-[[open]]:opacity-0"
        ></span
        ><span
          aria-hidden="true"
          class="absolute block h-0.5 w-5 transform bg-current transition duration-500 ease-in-out group-data-[closed]:translate-y-1.5 group-data-[enter]:-rotate-45 group-data-[leave]:!-rotate-0 group-[:not([open])]:translate-y-1.5 group-[[open]]:-rotate-45"
        ></span>
      </button>
      <dialog
        id="mobile-drawer"
        aria-labelledby="drawer-title"
        class="fixed top-[var(--header-h)] right-0 bottom-0 left-0 size-auto max-h-none max-w-none overflow-hidden bg-transparent not-open:hidden backdrop:bg-transparent"
      >
        <el-dialog-backdrop
          class="absolute top-0 right-0 bottom-0 left-0 bg-black/50 transition-opacity duration-500 ease-in-out data-closed:opacity-0"
        ></el-dialog-backdrop>
        <div
          tabindex="0"
          class="absolute inset-0 focus:outline-none"
        >
          <el-dialog-panel
            class="group/dialog-panel relative block size-full transform transition duration-500 ease-in-out data-closed:translate-x-full sm:duration-700"
          >
            <div class="relative flex h-full flex-col bg-white">
              <div class="flex h-full flex-col items-start gap-8 overflow-y-auto pt-4">
                <div class="flex min-h-px w-full min-w-px flex-1 flex-col items-start overflow-clip">
                  {% for menu in store.settings.menus.main_menu.items %}
                    {% if menu.items | length > 0 %}
                      <details class="group w-full">
                        <summary
                          class="text-primary flex w-full cursor-pointer items-center justify-between gap-1 px-6 py-2 text-base select-none"
                        >
                          <span class="text-primary min-h-px min-w-px flex-1 text-start text-[16px] leading-[24px]">
                            {{ menu.label }}
                          </span>
                          <svg
                            class="size-4 transition-transform group-open:rotate-90"
                            viewBox="0 0 16 16"
                            fill="none"
                          >
                            <path
                              d="M6 12l4-4-4-4"
                              stroke="currentColor"
                              stroke-width="2"
                              stroke-linecap="round"
                              stroke-linejoin="round"
                            />
                          </svg>
                        </summary>
                        <div class="mt-1 space-y-1 ps-8">
                          {% for submenu in menu.items %}
                            <a
                              href="{{ submenu.url | localized_url if submenu.resource_type != 'url' else submenu.url }}"
                              class="text-primary block px-6 py-2 text-base"
                            >
                              {{ submenu.label }}
                            </a>
                          {% endfor %}
                        </div>
                      </details>
                    {% else %}
                      <a
                        href="{{ menu.url | localized_url if menu.resource_type != 'url' else menu.url }}"
                        class="text-primary min-h-px w-full min-w-px px-6 py-2 text-start text-[16px] leading-[24px]"
                      >
                        {{ menu.label }}
                      </a>
                    {% endif %}
                  {% endfor %}
                </div>
                <div class="flex w-full flex-col items-start gap-[3px] border-t border-[#F2F2F2] pt-4 pb-6">
                  <button
                    type="button"
                    class="text-primary min-h-px w-full min-w-px px-6 py-2 text-start text-[16px] leading-[24px]"
                  >
                    Log in
                  </button>
                  <div class="flex w-full flex-col items-start gap-2.5 py-0 ps-6 pe-0">
                    <div class="flex w-full items-start gap-4">
                      <button
                        type="button"
                        class="text-primary flex items-center justify-center gap-1 py-2 text-[16px] leading-[24px]"
                      >
                        Riyadh
                        <svg
                          xmlns="http://www.w3.org/2000/svg"
                          width="16"
                          height="16"
                          viewBox="0 0 16 16"
                          fill="none"
                        >
                          <path
                            d="M4.7251 6.18231C4.74222 6.18232 4.7492 6.18527 4.75146 6.18622L4.77783 6.20575L7.64502 9.09052L7.99854 9.44598L8.354 9.09149L11.2388 6.20575C11.2466 6.19792 11.2525 6.19402 11.2554 6.19208C11.257 6.19096 11.2579 6.19031 11.2583 6.19012H11.2651C11.2834 6.1908 11.2983 6.19307 11.3276 6.22235C11.3426 6.23728 11.3462 6.24633 11.3472 6.24872C11.3481 6.25095 11.3511 6.25787 11.3511 6.27509C11.3511 6.29208 11.3481 6.29911 11.3472 6.30145C11.3462 6.30384 11.3426 6.31289 11.3276 6.32782L8.04834 9.60712C8.03231 9.62314 8.02229 9.62961 8.01904 9.63153C8.0165 9.63259 8.01164 9.63441 8.00049 9.63446C7.989 9.63446 7.98354 9.63261 7.98096 9.63153L7.95166 9.60712L4.65576 6.31122C4.64868 6.30413 4.64476 6.29909 4.64307 6.29657C4.64222 6.29531 4.64233 6.29409 4.64209 6.29364H4.64111C4.64105 6.29341 4.64128 6.29174 4.64111 6.29071C4.64079 6.28864 4.63986 6.28398 4.64014 6.27606C4.64115 6.24749 4.64677 6.23134 4.67236 6.20575C4.68728 6.19084 4.69635 6.18722 4.69873 6.18622C4.70113 6.18523 4.70822 6.18231 4.7251 6.18231Z"
