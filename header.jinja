                                  <button
                                    type="button"
                                    class="text-primary w-full px-6 py-2 text-start text-[16px] leading-[24px]"
                                  >
                                    Egypt
                                  </button>
                                  <button
                                    type="button"
                                    class="text-primary w-full px-6 py-2 text-start text-[16px] leading-[24px]"
                                  >
                                    Saudi Arabia
                                  </button>
                                  <button
                                    type="button"
                                    class="text-primary w-full px-6 py-2 text-start text-[16px] leading-[24px]"
