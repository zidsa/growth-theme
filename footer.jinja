            width="106"
            height="44"
            src="{{ settings.logo_mobile | default(settings.logo_desktop) | default(store.logo) }}"
            class="block md:hidden"
            alt="{{ store.name }}"
          />
        {% else %}
          <img
            width="106"
            height="44"
            src="{{ store.logo }}"
            alt="{{ store.name }}"
          />
