                  {{ store.settings.contact.phone }}
                </a>
              </li>
            {% endif %}
          </ul>
        </div>
      </div>
    </div>
    <div class="border-border-light mt-6 border-t pt-6">
      <div class="flex flex-col items-center justify-between space-y-4 lg:flex-row lg:space-y-0">
        <div class="text-foreground text-body2 flex items-center gap-x-2">
          <span>{{ store.settings.branding.copyrights | safe }}</span>
          <span>|</span>
          <div class="flex items-center gap-x-1">
            <span class="text-body2"> {{ _("Powered by") }} </span>
            <img
              width="54"
              height="20"
              src="{{ "images/zid-logo.svg" | asset_url }}"
              alt="Zid"
            />
          </div>
        </div>

        <div class="flex flex-wrap items-center gap-x-2">
          {% for shipping_method in store.settings.checkout.shipping_methods %}
            <a
              class="py-1"
              href="/shipping-and-payment"
            >
              <img
                src="{{ image_url(shipping_method.icon, w=48, h=48, q=100) }}"
                loading="lazy"
                alt="{{ shipping_method.name }}"
                class="h-6 max-w-13.5 rounded-sm object-contain"
              />
            </a>
          {% endfor %}

          {% for payment_method in store.settings.checkout.payment_methods %}
            <a
              class="py-1"
              href="/shipping-and-payment"
            >
              <img
                src="{{ image_url(payment_method.icon, w=48, h=48, q=100) }}"
                loading="lazy"
                alt="{{ payment_method.name }}"
                class="h-6 max-w-13.5 rounded-sm object-contain"
              />
            </a>
          {% endfor %}

          {% if store.settings.general.tax_settings.is_certificate_visible %}
            <a
              {% if store.settings.general.tax_settings.tax_registration_certificate %}
                href="{{ store.settings.general.tax_settings.tax_registration_certificate }}" target="_blank"
              {% endif %}
              class="py-1"
            >
              <img
                src="{{ 'images/vat-certificate.png' | asset_url }}"
                loading="lazy"
                alt="{{ _('VAT Certificate') }}"
                class="h-6 max-w-13.5 rounded-sm object-contain"
              />
            </a>
          {% endif %}

          {% if store.settings.contact.business_center %}
            <a
              href="https://eauthenticate.saudibusiness.gov.sa/certificate-details/{{ store.settings.contact.business_center }}"
              target="_blank"
              class="flex flex-col items-center py-1"
            >
              <img
                src="{{ 'images/business_center.png' | asset_url }}"
                loading="lazy"
                alt="{{ _('Business Center') }}"
                class="h-6 max-w-13.5 rounded-sm object-contain"
              />
              <div class="text-foreground text-caption">{{ store.settings.contact.business_center }}</div>
            </a>
          {% endif %}

          {% if store.settings.general.tax_settings.tax_number %}
            <span class="text-muted text-body2">
              {{ _("VAT number:") }} {{ store.settings.general.tax_settings.tax_number }}
            </span>
          {% endif %}
          {% if store.settings.general.commercial_registration_number %}
            <span class="text-muted text-body2">
              {{ _("The commercial register number:") }} {{ store.settings.general.commercial_registration_number }}
            </span>
          {% endif %}
        </div>
      </div>
    </div>
  </div>
</footer>
