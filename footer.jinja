    <div class="grid grid-cols-1 gap-8 md:grid-cols-[repeat(auto-fit,minmax(250px,1fr))]">
      <div class="space-y-4">
        {% if settings.logo_desktop or settings.logo_mobile %}
          <img
            width="106"
            height="44"
            src="{{ settings.logo_desktop | default(settings.logo_mobile) | default(store.logo) }}"
            class="hidden md:block"
            alt="{{ store.name }}"
          />
